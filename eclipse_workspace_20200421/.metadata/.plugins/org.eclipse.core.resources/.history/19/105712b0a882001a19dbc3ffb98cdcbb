package kk;
//SQ: 
public class K108FieldInit {

	char c;
	boolean b;
	float f;
	
	void printall() {
		System.out.println("c= " + c);
		System.out.println("b= " + b);
		System.out.println("f= " + f);
		
	}
	
	public static void main(String[] args) {
		
		K108FieldInit f = new K108FieldInit();
		
		f.printall();

	}

}
