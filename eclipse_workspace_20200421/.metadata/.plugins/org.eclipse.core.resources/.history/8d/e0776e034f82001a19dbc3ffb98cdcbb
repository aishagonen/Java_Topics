package ii;

public class I82MyField {

	int x;
	int y;
	
	public void doStuff(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public void display() {
		System.out.println(x + " " + y + " :");
	}
	
	public static void main(String[] args) {
		
		I82MyField m1 = new I82MyField();
		
		m1.x = 100;
		m1.y =200;
		
		System.out.println();
		
	

		
	}

}
