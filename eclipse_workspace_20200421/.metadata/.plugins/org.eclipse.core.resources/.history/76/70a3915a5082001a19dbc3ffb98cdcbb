package ii;
//SQ: 57 & 68
public class I84aVar {

	public static void main(String[] args) {

		int aVar = 9; 
		
		if (aVar++ < 10) {
			System.out.println(aVar + " Hello World!");
		}
		else {
			System.out.println(aVar + " Hello Universe!");
		}
		
	}
}






/*
 	What is the result if the integer A02aVar is 9?
	Answ: 10 Hello World!
	
	
 */